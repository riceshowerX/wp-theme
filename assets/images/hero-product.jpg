<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <defs>
    <linearGradient id="motorGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#34495e;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2c3e50;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="pumpGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2980b9;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#3498db;stop-opacity:1" />
    </linearGradient>
    <filter id="dropShadow" x="-20%" y="-20%" width="140%" height="140%">
      <feGaussianBlur in="SourceAlpha" stdDeviation="15" />
      <feOffset dx="10" dy="10" result="offsetblur" />
      <feComponentTransfer>
        <feFuncA type="linear" slope="0.4" />
      </feComponentTransfer>
      <feMerge>
        <feMergeNode />
        <feMergeNode in="SourceGraphic" />
      </feMerge>
    </filter>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="600" fill="#f5f7fa" />
  
  <!-- AC Motor -->
  <group transform="translate(200, 300)" filter="url(#dropShadow)">
    <!-- Motor Body --<path d="M-120,-80 L-120,80 L120,80 L120,-80 Z" fill="url(#motorGradient)" stroke="#34495e" stroke-width="3" />
    <!-- Motor End Caps --<circle cx="-120" cy="0" r="80" fill="url(#motorGradient)" stroke="#2c3e50" stroke-width="3" /</circle cx="120" cy="0" r="80" fill="url(#motorGradient)" stroke="#2c3e50" stroke-width="3" />
    <!-- Motor Shaft --<path d="M120,0 L180,0" stroke="#95a5a6" stroke-width="15" stroke-linecap="round" />
    <!-- Cooling Fins -->
    <path d="M-100,-80 L-100,-120" stroke="#2c3e50" stroke-width="5" />
    <path d="M-60,-80 L-60,-120" stroke="#2c3e50" stroke-width="5" />
    <path d="M-20,-80 L-20,-120" stroke="#2c3e50" stroke-width="5" />
    <path d="M20,-80 L20,-120" stroke="#2c3e50" stroke-width="5" />
    <path d="M60,-80 L60,-120" stroke="#2c3e50" stroke-width="5" />
    <path d="M100,-80 L100,-120" stroke="#2c3e50" stroke-width="5" />
    <path d="M-100,80 L-100,120" stroke="#2c3e50" stroke-width="5" />
    <path d="M-60,80 L-60,120" stroke="#2c3e50" stroke-width="5" />
    <path d="M-20,80 L-20,120" stroke="#2c3e50" stroke-width="5" />
    <path d="M20,80 L20,120" stroke="#2c3e50" stroke-width="5" />
    <path d="M60,80 L60,120" stroke="#2c3e50" stroke-width="5" />
    <path d="M100,80 L100,120" stroke="#2c3e50" stroke-width="5" />
  </group>
  
  <!-- Water Pump -->
  <group transform="translate(500, 320)" filter="url(#dropShadow)">
    <!-- Pump Casing --<path d="M-60,-50 C-60,-100 60,-100 60,-50 L80,-20 C80,30 -80,30 -80,-20 Z" fill="url(#pumpGradient)" stroke="#2980b9" stroke-width="3" />
    <!-- Pump Inlet/Outlet --<path d="M80,-20 L120,-20" stroke="#2980b9" stroke-width="15" stroke-linecap="round" />
    <path d="M-80,-20 L-120,-20" stroke="#2980b9" stroke-width="15" stroke-linecap="round" />
    <!-- Connection to Motor --<path d="M0,-50 L0,-80" stroke="#2c3e50" stroke-width="25" />
    <path d="M0,-80 L0,-120" stroke="#95a5a6" stroke-width="15" />
  </group>
  
  <!-- Titecho Logo Overlay -->
  <group transform="translate(400, 300)">
    <path d="M-80,-150 L80,-150 L80,-100 L-80,-100 Z" fill="rgba(255,255,255,0.8)" />
    <text x="0" y="-120" font-family="Arial, sans-serif" font-size="24" font-weight="bold" text-anchor="middle" fill="#2c3e50">TITECHO</text>
  </group>
</svg>